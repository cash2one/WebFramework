		/* for layout */
		.container {
			border: 1px solid #ABABAB;
			padding-left: 5px;
			-moz-box-shadow:1px 1px 3px #919191; 
			-webkit-box-shadow:1px 1px 3px #919191; 
			background: #FFFFCC;
			font:12px/16px arial,sans-serif;
		}

		.container .title {
			text-align: center;
			font-weight: bold;
			margin-bottom: 5px;
		}

		.graphics {
			width: 98%;
			margin-bottom: 10px;
		}

		.container .desc{
			margin: 0px;
			font: 11px/1.231 arial,helvetica,clean,sans-serif;
			line-height:10px;
		}

		#tabs-2 .container {
			margin-right: 5px;
			margin-bottom: 5px;
		}

		#content {
			width: 100%;
			height: 70%;
		}
